{
    "name": "@ephemeral/docs",
    "private": true,
    "version": "1.3.4",
    "description": "",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "module": "./dist/esm/index.js",
    "files": [
        "README.md",
        "LICENSE.md",
        "package.json",
