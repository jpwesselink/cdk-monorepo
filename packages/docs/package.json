    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@ephemeral/utils": "^1.2.1"
    },
    "devDependencies": {
        "@types/node": "^14.14.19",
        "markdown-inject": "^2.0.0",
        "tsup": "^4.14.0",
        "typescript": "~4.4.4"
    }
}
